{
  "name": "use-typical",
  "version": "1.3.0",
  "description": "A React Hook for typing animate",
  "keywords": [
    "react",
    "react-component",
    "use-typical",
    "typing",
    "animate"
  ],
  "homepage": "http://github.com/Turkyden/use-typical",
  "repository": {
    "type": "git",
    "url": "git@github.com:Turkyden/use-typical.git"
  },
  "bugs": {
    "url": "http://github.com/Turkyden/use-typical/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "ahooks": "^2.9.3",
    "antd": "^4.10.0",
    "husky": "^4.3.6",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.10",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
